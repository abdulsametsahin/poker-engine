# Example Environment Variables
# Copy this file to .env.local for local development overrides

# API Base URL (without /api suffix)
REACT_APP_API_URL=http://localhost:8080/api

# WebSocket URL
REACT_APP_WS_URL=ws://localhost:8080/ws

# Matchmaking countdown duration in seconds
# This is the time players see before the game starts after a match is found
REACT_APP_MATCHMAKING_COUNTDOWN_SECONDS=10
